    <style name="SpinnerTextSmall" parent="Widget.AppCompat.Light.DropDownItem.Spinner">
        <item name="android:paddingStart">1dp</item>
        <item name="android:paddingEnd">1dp</item>
        <item name="android:textColor">?attr/colorPrimaryDark</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal|italic</item>
        <item name="android:typeface">monospace</item>
    </style>

