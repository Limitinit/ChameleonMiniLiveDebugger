        <item name="android:colorAccent">?colorAccent</item>
    </style>

    <style name="ActionBarTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <item name="logo">?actionBarLeftLogoIcon</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:background">?actionBarBackgroundColor</item>
        <item name="android:textColorPrimary">#ffffff</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">10sp</item>
        <item name="android:subtitleTextColor">#dcdcdc</item>
        <item name="android:contentInsetEnd">5dp</item>
        <item name="android:contentInsetStart">7dp</item>
        <item name="android:contentInsetLeft">7dp</item>
        <item name="android:titleMarginStart">15dp</item>
        <item name="android:titleTextStyle">@style/Actionbar.TitleText</item>
        <item name="android:actionOverflowMenuStyle">@style/Actionbar.PopupTheme</item>
        <item name="android:subtitleTextStyle">@style/Actionbar.TitleText</item>
        <!-- Overflow menu ... icon color: -->
        <item name="android:textColorSecondary">#ffffff</item>
    </style>

    <style name="Actionbar.TitleText" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textColorPrimary">#ffffff</item>
        <item name="android:textSize">6sp</item>
    </style>

    <style name="Actionbar.PopupTheme" parent="Base.Widget.AppCompat.ActionButton.Overflow">
        <item name="android:enabled">true</item>
        <item name="android:visible">true</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:background">?colorAccentHighlight</item>
        <item name="android:title">Theme Selections:</item>
        <item name="android:checkedTextViewStyle">@style/MenuCheckedStyle</item>
        <item name="android:padding">0dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:itemPadding">0dp</item>
        <item name="android:textAppearanceListItemSmall">?attr/textAppearanceSmallPopupMenu</item>
        <item name="android:layout_margin">0dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginBottom">0dp</item>
        <item name="android:inset">0dp</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textStyle">bold|italic</item>
        <item name="android:drawableStart">@drawable/thememarker24</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:maxHeight">0dp</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="MenuCheckedStyle" parent="Actionbar.PopupTheme">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?colorAboutLinkColor</item>
        <item name="android:background">?colorAccent</item>
        <item name="android:textStyle">bold|italic</item>
        <item name="android:drawableRight">@drawable/menucheck48</item>
        <item name="android:itemTextAppearance">?android:attr/textAppearanceLarge</item>
    </style>

    <style name="GridButtons">
        <item name="android:background">?colorAccentLog</item>
        <item name="android:maxButtonHeight">8dp</item>
        <item name="android:layout_columnWeight">0.25</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:maxHeight">40dp</item>
        <item name="android:layout_weight">0.29</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textStyle">normal|bold</item>`
    </style>

    <style name="GridButtonsNoWidth">
        <item name="android:background">?colorAccentLog</item>
        <item name="android:maxButtonHeight">8dp</item>
        <item name="android:maxHeight">40dp</item>
        <item name="android:layout_weight">0.29</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textStyle">normal|bold</item>
    </style>

    <style name="ColorHighlightButtons">
        <item name="android:maxButtonHeight">8dp</item>
        <item name="android:layout_columnWeight">0.075</item>
        <item name="android:layout_width">15dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_weight">0.075</item>
        <item name="android:textSize">11sp</item>
