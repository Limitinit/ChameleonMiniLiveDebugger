        <item name="buttonStyle">@style/AboutButtonTheme</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="AboutButtonTheme" parent="AppTheme">
        <item name="android:textColor">?colorAboutTextColor</item>
        <item name="android:background">?colorAccentHighlight</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="LogDataUITheme" parent="AppTheme">
        <item name="android:background">?colorPrimaryDarkLog</item>
        <item name="android:colorPrimary">?colorPrimary</item>
